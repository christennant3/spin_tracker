  <ItemGroup>
    <PackageReference Include="Abp" Version="5.5.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\SpinTracker.Core\SpinTracker.Core.csproj" />
  </ItemGroup>
</Project>